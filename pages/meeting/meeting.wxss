/* pages/meeting/meeting.wxss */
@import "../../common.wxss";

page {
  height: 100%;
}

.content {
  width: 100%;
  position: absolute;
}

.content .footer {
  width: 100%;
  font-size: 24rpx;
  justify-content: flex-end;
  height: 150px;
  position: fixed;
  bottom: 0;
}

.content .footer .toolbar {
  /* border-top: 1px solid #A5A5A5; */
  width: 100%;
  height: 64px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.content .footer .mic {
  height: 100%;
  width: 44px;
  background-size: 50% 50%;
  border: 0;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url('data:image/png;base64,');
}

.content .footer .muted {
  opacity: 0.5;
}

.content .footer .log {
  height: 100%;
  width: 44px;
  background-size: 75% 50%;
  border: 0;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.content .footer .hangup {
  height: 100%;
  width: 44px;
  background-size: 75% 50%;
  border: 0;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.content .footer .camera{
  height: 100%;
  width: 44px;
  background-size: 75% 50%;
  border: 0;
  background-repeat: no-repeat;
  background-position: center;
  background-image:url('data:image/png;base64,')
}

.content .footer .makeup{
  height: 100%;
  width: 44px;
  background-size: 75% 50%;
  border: 0;
  background-position: center;
  background-repeat: no-repeat;
  background-image:url('data:image/png;base64,');
}

.content .makeup.off{
  opacity: 0.5;
}

.content .video-container {
  display: flex;
  width: 100%;
  overflow: hidden;
  flex-wrap: wrap;
}

.content .video-container .pusher-container{
  background: black;
  display: block;
  position: absolute;
}

.content .video-container .play-container{
  background: black;
  display: block;
  position: absolute;
}

.sud{
  background-color: #1B2A38;
  opacity:0.65; 
}